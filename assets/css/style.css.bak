body, h1, h2, h3, h4, h5, h6, p{
	margin: 0px;
}
.container{
	height: 100%;
	width: 75%;
	margin: 0px auto;
}
/* header section start */
header{
	height: 160px;
	width: 100%;
	background:#CCCCCC;
}
.header-section{
	padding: 30px 0px 0px 10px;
	background: #F5F5F5;
	box-sizing: border-box;
}
.header-section h1{
	margin-bottom: 10px;
	color: #323232;
	font-size: 38px;
	font-weight: 400;
	letter-spacing: 2px;
	font-family: "century gothic", arial, sans-serif;
}
.header-section h1 a{
	color: #00C6F0;
	text-decoration: none;
}
.header-section p{
	color: #999999;
	font-size: 15px;
	font-weight: 400;
	font-family: "century gothic", arial, sans-serif;
}
/* header section end */

/* nav section start */
nav{
	height: 50px;
	width: 100%;
	background:#CCCCCC;
}
.nav-section{
	padding: 10px 0px;
	background: #DDDDDD;
	box-sizing: border-box;
}
.nav-section ul{
	width: 50%;
	margin: 0px 0px 0px auto;
	padding: 0px;
	text-align: right;
}
.nav-section li{
	list-style: none;
	display: inline-block;
}
.nav-section li a{
	color: #000;
	padding: 6px 10px;
	margin-right: 10px;
	text-decoration: none;
	background: #00C6F0;
	display: inline-block;
	font-family: "trebuchet ms", sans-serif;
}
/* nav section end */

/* banner-section start */
.banner-section{
	height: 180px;
	width: 100%;
	background: tomato;
	margin-top: 20px;
}
.banner-content{
	background: url(../photos/banner.jpg) no-repeat;
	background-size: cover;
	border: 10px solid #DDDDDD;
	box-sizing: border-box;
}
